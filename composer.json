{
  "name": "fond-of-spryker/log",
  "description": "Extends spryker log module.",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    },
    {
      "name": "Felix Pfister",
      "email": "felix@pfister.de"
    }
  ],
  "require": {
    "php": ">=7.1",
    "spryker/log": "^3.7.0",
    "graylog2/gelf-php": "^1.5.5"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.14.7",
    "codeception/codeception": "^2.5",
    "mikey179/vfsstream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.1",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/"
    }
  }
}
